<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>CreateRealCustomerServlet</servlet-name>
    <servlet-class>servlets.RealCustomerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CreateLoanTypeServlet</servlet-name>
    <servlet-class>servlets.LoanTypeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CreateGrantConditionServlet</servlet-name>
    <servlet-class>servlets.GrantConditionServlet</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>CreateRealCustomerServlet</servlet-name>
    <url-pattern>/CreateRealCustomerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CreateLoanTypeServlet</servlet-name>
    <url-pattern>/CreateLoanTypeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CreateGrantConditionServlet</servlet-name>
    <url-pattern>/CreateGrantConditionServlet</url-pattern>
  </servlet-mapping>



</web-app>
